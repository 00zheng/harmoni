{
  "manifest_version": 3,
  "name": "Key & BPM Finder",
  "version": "0.1.0",
  "description": "Detect BPM and musical key from a tab's audio or an uploaded file.",
  "permissions": ["tabCapture", "activeTab"],
  "action": {
    "default_title": "Key & BPM Finder",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png"
  }
}
